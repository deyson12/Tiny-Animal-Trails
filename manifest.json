{
  "manifest_version": 3,
  "name": "Tiny Animal Trails",
  "version": "1.0",
  "description": "Tiny Animal Trails by Deyson.",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pet.js"],
      "css": ["pet.css"]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
